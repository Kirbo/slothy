{
  "name": "sloth",
  "description": "Changes your Slack status based on the SSID you're currently connected to.",
  "productName": "Sloth",
  "productUrl": "https://github.com/kirbo/sloth/releases",
  "product": {
    "GitLabRepository": "https://gitlab.com/kirbo/sloth",
    "GitHubRepository": "https://github.com/kirbo/sloth"
  },
  "author": "kirbownz@me.com",
  "version": "0.1.0",
  "private": true,
  "main": "public/electron.js",
  "scripts": {
    "build": "yarn react-build && yarn dist",
    "build-all": "yarn react-build && yarn dist-all",
    "dev": "nf start",
    "dist": "electron-builder",
    "dist-all": "electron-builder -mwl",
    "electron-start": "electron .",
    "electron-dev": "NODE_ENV=development ELECTRON_START_URL=http://localhost:5000 node src/electron-wait-react",
    "pack": "electron-builder --dir",
    "react-dev": "react-app-rewired start",
    "react-build": "react-app-rewired build",
    "react-test": "react-app-rewired test",
    "react-eject": "react-scripts eject"
  },
  "build": {
    "appId": "com.github.kirbo.sloth",
    "asar": true,
    "files": [
      ".env",
      "build"
    ],
    "mac": {
      "category": "public.app-category.utilities"
    },
    "win": {
      "target": [
        "nsis"
      ]
    },
    "protocols": [
      {
        "name": "sloth2",
        "role": "Viewer",
        "schemes": [
          "sloth2",
          "sloth2s"
        ]
      }
    ]
  },
  "homepage": "./",
  "eslintConfig": {
    "extends": "react-app"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.15",
    "@fortawesome/free-brands-svg-icons": "5.7.2",
    "@fortawesome/free-regular-svg-icons": "5.7.2",
    "@fortawesome/free-solid-svg-icons": "5.7.2",
    "@fortawesome/react-fontawesome": "0.1.4",
    "antd": "3.15.2",
    "axios": "0.18.0",
    "babel-plugin-import": "1.11.0",
    "customize-cra": "0.2.12",
    "dotenv": "6.2.0",
    "electron-json-storage": "4.1.6",
    "electron-log": "3.0.1",
    "electron-updater": "4.0.6",
    "less": "3.9.0",
    "less-loader": "4.1.0",
    "node-emoji": "1.10.0",
    "node-wifi": "https://github.com/kirbo/node-wifi#master",
    "nodemon": "1.18.10",
    "react": "16.8.4",
    "react-app-rewired": "2.1.0",
    "react-axe": "3.0.2",
    "react-dom": "16.8.4",
    "react-scripts": "2.1.8",
    "request": "2.88.0",
    "slack": "11.0.2",
    "styled-components": "4.1.3"
  },
  "devDependencies": {
    "electron": "4.0.6",
    "electron-builder": "20.38.5",
    "foreman": "3.0.1",
    "react": "16.8.3",
    "react-dom": "16.8.3",
    "react-scripts": "2.1.5",
    "styled-components": "4.1.3",
    "typescript": "3.3.3333"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
